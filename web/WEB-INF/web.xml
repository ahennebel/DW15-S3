<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>traitementMenu</servlet-name>
        <servlet-class>gestionetudiants.traitementMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajoutetudiant</servlet-name>
        <servlet-class>gestionetudiants.ajoutetudiant</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajoutpromotion</servlet-name>
        <servlet-class>gestionetudiants.ajoutpromotion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>affichagepromotion</servlet-name>
        <servlet-class>gestionetudiants.affichagepromotion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajoutetudiantpromo</servlet-name>
        <servlet-class>gestionetudiants.ajoutetudiantpromo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listingetudiantspromo</servlet-name>
        <servlet-class>gestionetudiants.listingetudiantspromo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listeetupromo</servlet-name>
        <servlet-class>gestionetudiants.listeetupromo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>traitementMenu</servlet-name>
        <url-pattern>/traitementMenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajoutetudiant</servlet-name>
        <url-pattern>/ajoutetudiant</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajoutpromotion</servlet-name>
        <url-pattern>/ajoutpromotion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>affichagepromotion</servlet-name>
        <url-pattern>/affichagepromotion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajoutetudiantpromo</servlet-name>
        <url-pattern>/ajoutetudiantpromo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listingetudiantspromo</servlet-name>
        <url-pattern>/listingetudiantspromo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listeetupromo</servlet-name>
        <url-pattern>/listeetupromo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
